{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "backdrop-filter",
          "scrollbar-width",
          "scrollbar-color",
          "scrollbar-width: thin"
        ]
      }
    ],
    "meta-viewport": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "script[type=importmap]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": "off"
  }
}